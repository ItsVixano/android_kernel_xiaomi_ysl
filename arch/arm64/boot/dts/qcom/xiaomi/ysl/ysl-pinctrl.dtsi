				};

				config {
					pins = "gpio64";
					drive-strength = <8>;
					bias-pull-down;
					 output-low;
				};
			};
		};
		
	pmx_ts_int_release {
                            ts_as_int: ts_as_int {
                                mux {
                                    pins = "gpio65";
                                    function = "gpio";
                                };

                                config {
                                    pins = "gpio65";
                                    drive-strength = <8>;
                                    input-enable;
                                    bias-disable;
                                };
                            };
                        };

                        pmx_ts_reset_release {
                            ts_as_reset: ts_as_reset {
                                mux {
                                    pins = "gpio64";
                                    function = "gpio";
                                };

                                config {
                                    pins = "gpio64";
                                    drive-strength = <8>;
                                    input-enable;
                                    bias-disable;
                                };
                            };
                        };
		// add by wangyibo for sar ant detect by wangyibo at 20190108 start
